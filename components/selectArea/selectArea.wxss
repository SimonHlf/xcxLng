view.areaViewTxt{
  font-size: 28rpx;
  text-align: center;
}
.title{
  width: 88%;
  text-align: right;
  padding-left: 5%;
  float: left;
  color: #121212;
}
.title.noSel{
	color: #808080;
}
.cancel-click{
  display: flex;
  justify-content: space-between;
  padding: 10rpx 0;
  border-top: 1px solid #D8D8D8;
  border-bottom: 1px solid #D8D8D8;
}
.cancel-click text:nth-child(1){
  padding: 0 60rpx;
  font-size: 30rpx;
  color: rgb(153,153,153);
}
.cancel-click text:nth-child(2){
  padding: 0 60rpx;
  font-size: 30rpx;
  color: rgb(25, 163, 24);
}
.city-body{
  width: 100%;
  position: fixed;
  left: 0;
  bottom:0;
  background-color: #fff;
  z-index: 999;
  transition: ease .3s;
  transform: translateY(100%);
}
.city-body.active{
  transform: translateY(0);
}
.mask{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, .5);
}