page{
	background: #efefef;
	margin-bottom: 120rpx;
}
.userHead{
	width: 750rpx;
	height: 120rpx;
	position: relative;
}
.userHead image.qzBg{
	width: 100%;
	height: 100%;
	position: absolute;
	left: 0;
	top: 0;
	z-index: 1 ;
}
.headImg{
	width: 136rpx;
	height: 136rpx;
	border-radius: 50%;
	background: #fff;
	position: absolute;
	right: 30rpx;
	bottom: -68rpx;
	z-index: 2;
	text-align: center;
	display: flex;
	justify-content: center;
	align-items: center;
}
.headImg image{
	width: 120rpx;
	height: 120rpx;
	border-radius: 50%;
	overflow: hidden;
}
.userInfo{
	background: #fff;
	padding:20rpx 30rpx;
	position: relative;
	margin-bottom: 20rpx;
}
.topUserInfo{
	height: 50rpx;
	line-height: 50rpx;
	margin-bottom: 5rpx;
}
.userName{
	font-size: 32rpx;
	color: #121212;
	font-weight: 620;
	float: left;
}
.renzheng{
	font-size: 24rpx;
	color: #ffc107;
	float: left;
	margin:0rpx 0 0 10rpx;
}
.renzheng i{
	font-size: 28rpx;
}
.basicInfo{
	height: 40rpx;
	line-height: 40rpx;
}
.basicInfo view{
	float: left;
	font-size: 26rpx;
	color:#606060 ;
	margin-right: 15rpx;
}
.infoItemWrap{
	margin-top: 15rpx;
}
.infoItem{
	width: 50%;
	height: 50rpx;
	line-height: 50rpx;
	float: left;
}
.infoItem view.infoTit{
	float: left;
	width: 120rpx;
	color: #a9a9a9;
	font-size: 26rpx;
}
.infoItem view.infoCon{
	width: 65%;
	height: 100%;
	float: left;
	font-size: 26rpx;
	color: #121212;
}
.allInfoTit{
	height: 50rpx;
	line-height: 50rpx;
	font-size: 32rpx;
	color: #121212;
	padding: 10rpx 0 0;
}
.detInfo{
	background: #fff;
}
.detInfoItem{
	height: 80rpx;
	line-height: 80rpx;
}
.detInfoItem_spec{
	margin-top:15rpx
}
.detInfoItem view.detInfoTit,
.detInfoItem_spec view.detInfoTit{
	width: 130rpx;
	height: 100%;
	font-size: 26rpx;
	color: #606060;
}
.detInfoItem view.detInfoTit,
.detInfoItem view.detInfoCon{
	float: left;
}
.detInfoItem view.detInfoCon,
.detInfoItem_spec view.detInfoCon{
	width: 80%;
	font-size: 26rpx;
	color: #121212;
}
.detInfoItem view.detInfoCon{
	height: 100%;
}
.detInfoItem_spec{
	position: relative;
	padding-bottom: 20rpx;
}
.detInfoItem_spec view.detInfoTit{
	position: absolute;
	left: 0;
	top: 0;
	height: 100%;
	display: flex;
}
.detInfoItem_spec view.detInfoCon{
	margin-left:130rpx ;
}
.botFun{
	width: 100%;
	height: 100rpx;
	background: #fff;
	position: fixed;
	left: 0;
	bottom: 0;
	z-index: 100;
}
.botFun .focusView{
	width: 120rpx;
	float: left;
	margin-left: 30rpx;
	text-align: center;
	position: relative;
}
.botFun .focusView:last-child{
	margin-left: 0;
}
.botFun .focusView view{
	height: 50rpx;
	line-height: 50rpx;
}
.botFun .focusView view i.icon-guanzhu,
.botFun .focusView view i.icon-wodexiaoxi{
	font-size: 34rpx;
	color:#606060 ;
}
.botFun .focusView view i.icon-guanzhu1{
	font-size: 38rpx;
	color: #d32f2f;
}
.botFun .focusView view.iconTxt{
	color:#606060 ;
	font-size: 28rpx;
	margin-top: -10rpx;
}
.botFun .focusView view:first-child{
	margin-top: 10rpx;
}
.buyGasBtn,
.notBuyGasBtn{
	width: 400rpx;
	height: 66rpx;
	line-height: 66rpx;
	color: #fff;
	font-size: 28rpx;
	text-align: center;
	float: right;
	margin-right: 30rpx;
	margin-top: 18rpx;
	border-radius: 100rpx;
}
.buyGasBtn{
	background:#ffc107 ;
}