page{
	background: #efefef;
}
.msgListItem{
	margin-bottom: 35rpx;
}
.msgListItem .pubTime{
	width: 165rpx;
	height: 40rpx;
	line-height: 40rpx;
	border-radius: 100rpx;
	background: #cccccc;
	color: #fff;
	font-size: 24rpx;
	text-align: center;
	margin:20rpx auto 0;
}
.msgListCon{
	padding: 20rpx 15rpx;
	background: #fff;
	font-size: 28rpx;
	color: #121212;
	border-radius: 8rpx;
	margin-top: 15rpx;
	position: relative;
}
.innerCon.hasMore{
	width: 94%;
}
.msgListCon .moreBtn{
	width: 6%;
	height: 100%;
	text-align: right;
	position: absolute;
	right:15rpx;
	top: 0;
	display: flex;
	align-items: center;
	justify-content: flex-end;
}
.msgListCon i{
	font-size: 28rpx;
	color:#cccccc ;
	
}