page{
	background: #efefef;
}
.mainHeadImg{
	width: 100%;
	height: 400rpx;
	background: pink;
}
.mainHeadImg imgage{
	width: 100%;
	height: 100%;
}
.sellerInfo{
	background: #fff;
	margin-bottom: 20rpx;
}
.topSellerInfo{
	height: 80rpx;
	line-height: 80rpx;
}
.topSellerInfo .compName{
	font-size: 30rpx;
	color: #121212;
	width: 100%; 
}
.priceItem{
	padding-bottom: 15rpx;
}
.priceNum{
	float: left;
	color: #606060;
	line-height: 40rpx;
	margin-right: 30rpx;
}
.priceNum text.rmb{
	color:#d32f2f ;
	font-size: 24rpx;
}
.priceNum text.price{
	color:#d32f2f ;
	font-size: 30rpx;
	font-weight: 620;
}
.priceNum text:last-child{
	font-size: 24rpx;
}
.cd{
	float: right;
	color: #a9a9a9;
	line-height: 40rpx;
	font-size: 24rpx;
	margin-right: 30rpx;
}
.sellerTel{
	background: #fff; 
	padding: 15rpx 30rpx;
}
.sellerTel .sellerImg{
	width:80rpx;
	height: 80rpx;
	border-radius: 50%;
	background: pink;
	float: left;
}
.sellerTel .sellerImg image{
	width:80rpx;
	height: 80rpx;
	border-radius: 50%;
	overflow: hidden;
}
.rqDevCompName{
	width: 75%;
	height: 80rpx;
	float: left;
	margin-left: 15rpx;
	color: #121212;
	font-size: 28rpx;
	background: yellow;
}
.phoneCall{
	float: right;
	width: 10%;
	height: 80rpx;
	line-height: 80rpx;
	text-align: right;
}
.phoneCall i{
	color: #1cb022;
	font-size: 40rpx;
}
.lxrNameTel,
.rqDevName{
	height: 40rpx;
	line-height: 40rpx;
	font-size: 28rpx;
	color: #121212;
}
.lxrNameTel text:last-child{
	margin-left: 15rpx;
}
.proBasicInfo{
	background: #fff;
	margin-top: 20rpx;
}
.proTit{
	position: relative;
	height: 80rpx;
	line-height: 80rpx;
	font-size: 30rpx;
	color: #121212;
	border-bottom: 1rpx solid #e8e8e8;
}
.proTit view.lineView{
	width: 8rpx;
	height: 36rpx;
	background: #3c7de9;
	position: absolute;
	left: 0;
	top: 22rpx;
}
.proTit view:last-child{
	margin-left:20rpx;
}
.basicInfoItem{
	height: 80rpx;
	line-height: 80rpx;
	font-size: 28rpx;
	color: #121212;
}
.basicInfoItem text:first-child{
	color: #606060;
	margin-right: 15rpx;
}